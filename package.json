    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.2",
    "mobx": "^5.8.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rollup": "^1.1.0",
    "source-map-support": "^0.5.10",
    "ts-node": "^7.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {
    "mobx": "^4.3.1 || ^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonk/mobx-to-jsonpatch.git"
  },
  "bugs": {
    "url": "https://github.com/jasonk/mobx-to-jsonpatch/issues"
